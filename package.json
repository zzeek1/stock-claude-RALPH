{
  "name": "stock-claude",
  "version": "1.0.0",
  "description": "股票交易日志与复盘分析系统",
  "main": "dist/main/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "cross-env VITE_DEV_PORT=5173 tsc -p tsconfig.main.json --watch",
    "dev:renderer": "cross-env VITE_DEV_PORT=5173 vite",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build": "npm run build:main && npm run build:renderer",
    "build:check": "npm run build:main 2>&1 | tee build-check.log",
    "start": "electron .",
    "start:check": "npm run build:check && if errorlevel 1 (echo 构建失败！请检查错误后重试 && type build-check.log && exit /b 1) else (echo 构建成功，正在启动... && electron .)",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "@anthropic-ai/sdk": "^0.39.0",
    "antd": "^5.22.0",
    "dayjs": "^1.11.13",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "longbridge": "^0.2.77",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.0",
    "sql.js": "^1.11.0",
    "uuid": "^11.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jspdf": "^1.3.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.1.0",
    "cross-env": "^10.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.0",
    "jsdom": "^28.1.0",
    "terser": "^5.46.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
